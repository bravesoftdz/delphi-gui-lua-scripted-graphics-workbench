      'red = color(255,0,0)'
      'green = color(0,255,0)'
      'blue = color(0,0,255)'
      ''
      'x = 1'
      'while x < 100 do'
      '  y = 1'
      '  while y < 100 do'
      '    point(x,y,color(random(255),random(255),random(255)))'
      '    point(x+100,y,red)'
      '    point(x,y+100,green)'
      '    point(x+100,y+100,blue)'
      '    y = y + 1'
      '  end'
      '  x = x + 1'
      'end'
      ''
      'function bar(x,y)'
      '  i = 1'
      '  r,g,b = random(128)+128,random(128)+128,random(128)+128'
      '  while i < 16 do'
      '    line(0+x,0+i+y,100+x,0+i+y+20,color(r-i*8,g-i*8,b-i*8))'
      '    i = i + 1'
      '  end'
      'end'
      ''
      'x = 1'
      'while x < 20 do'
      '  bar(random(100)+200,random(200))'
      '  x = x + 1'
      'end'
      ''
      'x = 50'
      'while x < 150 do'
      '  y = 50'
      '  while y < 150 do'
      
        '    point(x,y,color(pixel(x,y).red/2,pixel(y,x).green/2,pixel(y,' +
        'x).blue/2))'
      '    y = y + 1'
      '  end'
      '  x = x + 1'
      'end')
